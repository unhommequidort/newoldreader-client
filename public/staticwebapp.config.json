{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
        "rewrite": "/index.html",
        "statusCode": 200
    }
},
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}